<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        
<mapper namespace="com.shsxt.dao.UserRoleDao">

<insert id="insertBatch">
insert into t_user_role (user_id,role_id,is_valid,create_date,update_date)
values
<foreach collection="userRoles" item="item" separator=",">
(#{item.userId},#{item.roleId},#{item.isValid},#{item.createDate},#{item.updateDate})
</foreach>
</insert>

</mapper>
